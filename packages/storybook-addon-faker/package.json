{
  "name": "storybook-addon-faker",
  "private": false,
  "version": "0.5.4",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./babel": {
      "import": "./dist/babel/index.js",
      "require": "./dist/babel/index.cjs"
    },
    "./preset": {
      "import": "./dist/preset/index.js",
      "require": "./dist/preset/index.cjs"
    }
  },
  "sideEffects": false,
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "build": "vite build",
    "dev": "yarn build --watch",
    "lint": "TIMING=1 eslint src",
    "format": "yarn lint --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "@storybook/node-logger": "^6.5.13",
    "chromatic": "^6.11.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.5.0",
    "@storybook/addon-essentials": "^6.5.8",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/react": "^6.5.8",
    "@types/jest": "^26.0.22",
    "@types/node": "^18.14.2",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.1",
    "babel-plugin-tester": "^10.1.0",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "scripts": "*",
    "tsconfig": "*",
    "typescript": "^4.5.3",
    "vite": "^4.1.4",
    "vite-plugin-dts": "^2.0.2"
  },
  "peerDependencies": {
    "@storybook/addons": "^6.5.8",
    "@storybook/api": "^6.5.8",
    "@storybook/components": "^6.5.8",
    "@storybook/core-events": "^6.5.8",
    "@storybook/theming": "^6.5.8",
    "react": "^16.8.0 || ^17.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "jest": {
    "preset": "scripts/jest/node"
  },
  "repository": "KevinMind/storybook-addon-faker",
  "author": "kevin meinhardt <kmeinhardt7@gmail.com>",
  "keywords": [
    "addon",
    "storybook",
    "faker",
    "mock",
    "essentials",
    "data-state"
  ],
  "storybook": {
    "displayName": "Storybook addon faker",
    "supportedFrameworks": [
      "react"
    ],
    "icon": "https://user-images.githubusercontent.com/321738/63501763-88dbf600-c4cc-11e9-96cd-94adadc2fd72.png"
  }
}
