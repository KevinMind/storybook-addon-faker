{
  "name": "@storybook-addon-faker/babel",
  "version": "0.0.3",
  "main": "./dist/index.js",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "test": "jest",
