{
  "name": "@storybook-addon-faker/addon",
  "private": false,
  "version": "0.2.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "test": "jest --passWithNoTests ",
    "build": "tsup src/** --format esm,cjs --external react",
    "dev": "yarn build --watch",
    "lint": "TIMING=1 eslint src",
    "format": "yarn lint --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "chromatic": "^6.11.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.5.0",
    "@storybook/addon-essentials": "^6.5.8",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/react": "^6.5.8",
    "@types/jest": "^26.0.22",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.1",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "scripts": "*",
    "tsconfig": "*",
    "tsup": "^5.10.1",
    "typescript": "^4.5.3"
  },
  "peerDependencies": {
    "@storybook/addons": "^6.5.8",
    "@storybook/api": "^6.5.8",
    "@storybook/components": "^6.5.8",
    "@storybook/core-events": "^6.5.8",
    "@storybook/theming": "^6.5.8",
    "react": "^16.8.0 || ^17.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "jest": {
    "preset": "scripts/jest/node"
  },
  "repository": "KevinMind/storybook-addon-faker",
  "author": "kevin meinhardt <kmeinhardt7@gmail.com>",
  "auto": {
    "plugins": [
      "npm",
      "released"
    ],
    "onlyPublishWithReleaseLabel": true
  }
}
