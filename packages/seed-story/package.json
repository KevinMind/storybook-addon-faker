    "jest": "^26.6.3",
    "scripts": "*",
    "tsconfig": "*",
    "tsup": "^5.10.1",
    "typescript": "^4.5.3"
  },
  "jest": {
    "preset": "scripts/jest/node"
  },
  "peerDependencies": {
    "@faker-js/faker": "^7.5.0",
    "chromatic": "^6.10.1"
  }
